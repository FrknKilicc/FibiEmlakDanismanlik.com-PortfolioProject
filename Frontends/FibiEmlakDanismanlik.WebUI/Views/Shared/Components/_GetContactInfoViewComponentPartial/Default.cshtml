    
@model List<ResultContactDto>    


<!--Page Title-->
<section class="page-title centred" style="background-image: url(https://images.stockcake.com/public/e/3/0/e30fd6b9-27e3-4bf8-95ff-ba161c9d5931_large/real-estate-transaction-stockcake.jpg);">
    <div class="auto-container">
        <div class="content-box clearfix">
            <h1>İletişim</h1>
            <ul class="bread-crumb clearfix">
                <li><a @Url.Action("Index","UILayout")">Anasayfa</a></li>
                <li>İletişim</li>
            </ul>
        </div>
    </div>
</section>
<!--End Page Title-->
<!-- contact-info-section -->
<section class="contact-info-section sec-pad centred">
    <div class="auto-container">
        <div class="sec-title">
            <h5>Bizimle İletişime geç</h5>
            <h2>İletişim Adresleri</h2>
        </div>
        @foreach(var item in Model)
        {
             <div class="row clearfix">
            <div class="col-lg-4 col-md-6 col-sm-12 info-block">
                <div class="info-block-one">
                    <div class="inner-box">
                        <div class="icon-box"><i class="icon-32"></i></div>
                        <h4>Email Address</h4>
                            <p><a href="mailto:info@example.com">@item.EMail1</a><br /><a href="mailto:info@example.com">@item.EMail2</a></p>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 col-sm-12 info-block">
                <div class="info-block-one">
                    <div class="inner-box">
                        <div class="icon-box"><i class="icon-33"></i></div>
                        <h4>Telefon</h4>
                            <p><a href="tel:+23055873407">+(90)@item.PhoneNumber1</a><br /><a href="tel:+23055873408">+(90)@item.PhoneNumber2</a></p>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 col-sm-12 info-block">
                <div class="info-block-one">
                    <div class="inner-box" style="height:344px;">
                        <div class="icon-box"><i class="icon-34"></i></div>
                        <h4>Ofis Adresi</h4>
                        <p>@item.OfficeAddress <br />Türkiye</p>
                    </div>
                </div>
            </div>
        </div>
            
        }
       
    </div>
</section>
<!-- contact-info-section end -->
<!-- contact-section -->

<!-- contact-section end -->
