@model List<ResultMainBannerDto>


<section class="banner-style-two centred">
    <div class="banner-carousel owl-theme owl-carousel owl-nav-none">
        @foreach (var item in Model)
        {
            <div class="slide-item">
                <div class="image-layer" style="background-image:url(@item.MainBannerUrl)"></div>
                <div class="auto-container">
                    <div class="content-box">
                        <h2>@item.MainBannerDesc</h2>
                        <p>Amet consectetur adipisicing elit sed do eiusmod.</p>
                    </div>
                </div>
            </div>

        }

    </div>
</section>