        $(document).ready(function () {
            $('.tab-btn').click(function () {
                $('.tab-btn').removeClass('active-btn');
                $(this).addClass('active-btn');
                debugger;
                let methodName = $(this).attr('method');

                $.ajax({
                 
                    url: `/Home/${methodName}`, 
                    type: 'GET',
                    success: function (result) {
                        $('#props').html(result); 
                        console.log(result);
                       
                    error: function (xhr, status, error) {
